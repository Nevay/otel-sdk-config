                "Nevay\\OTelSDK\\Configuration\\ConfigEnv\\Metrics\\ExemplarFilterLoaderAlwaysOn",
                "Nevay\\OTelSDK\\Configuration\\ConfigEnv\\Metrics\\ExemplarFilterLoaderTraceBased",
                "Nevay\\OTelSDK\\Configuration\\ConfigEnv\\Metrics\\MetricReaderLoaderConsole",
                "Nevay\\OTelSDK\\Configuration\\ConfigEnv\\Metrics\\MetricReaderLoaderNone",
                "Nevay\\OTelSDK\\Configuration\\ConfigEnv\\Metrics\\MetricReaderLoaderOtlp",
                "Nevay\\OTelSDK\\Configuration\\ConfigEnv\\Metrics\\MetricReaderLoaderPrometheus",
                "Nevay\\OTelSDK\\Configuration\\ConfigEnv\\Logs\\LogRecordProcessorLoaderConsole",
                "Nevay\\OTelSDK\\Configuration\\ConfigEnv\\Logs\\LogRecordProcessorLoaderNone",
                "Nevay\\OTelSDK\\Configuration\\ConfigEnv\\Logs\\LogRecordProcessorLoaderOtlp"
            ]
        }
    }
}
